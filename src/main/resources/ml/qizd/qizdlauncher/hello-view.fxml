<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="480.0" maxWidth="640.0" minHeight="480.0" minWidth="640.0" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ml.qizd.qizdlauncher.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
      <children>
         <TextField fx:id="login" promptText="Login" />
         <PasswordField fx:id="password" promptText="Password" />
         <Button fx:id="sign_in" mnemonicParsing="false" onAction="#onSignInButtonCLick" text="Sign in" />
      </children>
   </VBox>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Download Minecraft" />
   <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" />
</VBox>
